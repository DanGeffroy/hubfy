<!doctype html>
<html>

<head>
    <base href="/">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no" />
    <title>hubfy</title>

    <link rel="shortcut icon" href="/logo.png" type="image/x-icon">
    <link rel="icon" href="/logo.png" type="image/x-icon">


    <!-- CSS  -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection" />
    <link href="css/style.css" type="text/css" rel="stylesheet" media="screen,projection" />

    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <!-- 1. Load libraries -->
  <script src="node_modules/zone.js/dist/zone.js"></script>
  <script src="node_modules/reflect-metadata/Reflect.js"></script>
  <script src="node_modules/systemjs/dist/system.src.js"></script>


    <!-- 2. Configure SystemJS -->
    <script>
      System.config({
        defaultJSExtensions: true,
        packages: {
          '@angular/compiler': { main: 'index.js', defaultExtension: 'js' },
          '@angular/common': { main: 'index.js', defaultExtension: 'js' },
          '@angular/core': { main: 'index.js', defaultExtension: 'js' },
          '@angular/platform-browser-dynamic': { main: 'index.js', defaultExtension: 'js' },
          '@angular/platform-browser': { main: 'index.js', defaultExtension: 'js' }
        },
        map: {
          '@angular': 'node_modules/@angular',
          'angular2-grid': 'node_modules/angular2-grid/dist/NgGrid',
          'rxjs': 'node_modules/rxjs'
        }
      });
      System.import('app/boot')
            .then(null, console.error.bind(console));
    </script>
</head>

<body>
    <nav class="grey darken-4" role="navigation">
        <div class="nav-wrapper container">
            <a id="logo-container" href="#" class="brand-logo">Hubfy</a>
            <ul class="right hide-on-med-and-down">
                <li><a href="/logout">Logout <span class="fa fa-sign-in"></span></a></li>
            </ul>

            <ul id="nav-mobile" class="side-nav grey darken-4 white-text">
                <li><a href="/logout">Logout <span class="fa fa-sign-in"></span></a></li>
            </ul>
            <a href="#" data-activates="nav-mobile" class="button-collapse"><i class="material-icons">menu</i></a>
        </div>
    </nav>

    <div class="container">
        <div class="fixed-action-btn  horizontal click-to-toggle" style="top: 60px; right: 70px;">
            <a class="btn-floating btn-large waves-effect orange darken-3 waves-light">
                <i class="large material-icons">add</i>
            </a>
            <ul>
                <% var url = "/youtubeplayer/add?_id="+ user._id +"&url=PLfZeR4pSmgNb_qJGpWcDsCSwkNiZUCkjO" %>
                <li><a id="youtubeAddButton" href=<%= url %> class="btn-floating red"><i class="material-icons">play_arrow</i></a></li>
            </ul>
        </div>
        <div class="row">
            <my-app>Loading...</my-app>
        </div>
    </div>

    <footer class="page-footer grey darken-4">
        <div class="container">
            <div class="row">
                <div class="col l6 s12">
                    <h5 class="white-text">Contact</h5>
                    <p class="grey-text text-lighten-4">I'm a junior developer, and i'm working on this project on my free time. Feel free to contact me on github if you have any sugestion.</p>


                </div>
                <div class="col l3 offset-l3 s12">
                    <h5 class="white-text"><a href="https://github.com/adNNNN">my GitHub :D</a></h5>
                </div>

            </div>
        </div>
        <div class="footer-copyright">
            <div class="container">
                Made by <a class="brown-text text-lighten-3" href="https://github.com/DanGeffroy/">Dan Geffroy</a>
            </div>
        </div>
    </footer>



    <!--  Scripts-->
    <script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="js/materialize.js"></script>
    <script src="js/lodash.js"></script>
    <script src="js/jquery-ui.js"></script>
    <script src="js/init.js"></script>
</body>

</html>
